algoritmo "semnome"

var
   Nota1, Nota2, Media: Real
inicio
      Escreva("Primeira nota: ")
      Leia(Nota1)
      Escreva("Segunda nota: ")
      Leia(Nota2)
      Media <- (Nota1 +  Nota2) / 2
      EscrevaL("A média entre as notas", Nota1, " e", Nota2, " é igual a:", Media)
fimalgoritmo
