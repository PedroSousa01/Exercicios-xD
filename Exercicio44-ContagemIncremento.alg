algoritmo "semnome"

var
   val, ult, inc: Inteiro
inicio
      Escreva("Digite o primeiro Valor: ")
      Leia(val)
      Escreva("Digite o último Valor: ")
      Leia(ult)
      Escreva("Digite o incremento: ")
      Leia(inc)
      Enquanto (val <= ult) faca
         Escreva(val)
         val <- val + inc
      FimEnquanto
      Escreva(" Acabou!")
fimalgoritmo
